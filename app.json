{
  "expo": {
    "name": "viafarm",
    "slug": "viafarm",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/via-farm-img/icons/logo.png",
    "scheme": "viafarm",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyCG4EdVELDakzLL4zmxPzC9JKASgqFnvZc"
      },
      "bundleIdentifier": "com.devsanjaychauhan.viafarm"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/via-farm-img/icons/logo.png",
        "backgroundImage": "./assets/via-farm-img/icons/logo.png",
        "monochromeImage": "./assets/via-farm-img/icons/logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.devsanjaychauhan.viafarm",
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCG4EdVELDakzLL4zmxPzC9JKASgqFnvZc"
        }
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/via-farm-img/icons/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/via-farm-img/icons/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      "expo-font",
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermissions": "Allow $(PRODUCT_NAME) to access your location"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c0583762-adbb-40cb-adf2-37c0b7cecbd1"
      }
    },
    "owner": "sagjay"
  }
}
